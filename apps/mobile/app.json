{
  "expo": {
    "name": "MyDailyOps",
    "slug": "mydailyops-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.mydailyops.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mydailyops.mobile"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "scheme": "mydailyops",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
